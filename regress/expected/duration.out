-- Infinity
-- Create helper tables
CREATE TEMP TABLE inf_table(d duration);
INSERT
INTO
	inf_table
VALUES
	('infinity'), ('-infinity'), ('999 hours'), ('-999 hours');
-- Input
SELECT duration 'infinity';
 duration 
----------
 infinity
(1 row)

SELECT duration '-infinity';
 duration  
-----------
 -infinity
(1 row)

-- Comparison
SELECT
	t1.d AS a,
	t2.d AS b,
	t1.d < t2.d AS lt,
	t1.d <= t2.d AS le,
	t1.d > t2.d AS gt,
	t1.d >= t2.d AS ge,
	t1.d = t2.d AS eq,
	t1.d != t2.d AS ne
FROM
	inf_table AS t1, inf_table AS t2;
        a        |        b        | lt | le | gt | ge | eq | ne 
-----------------+-----------------+----+----+----+----+----+----
